{
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "ContentManagement.Infrastructure.Outbox": "Warning",
        "ContentManagement.Infrastructure.Inbox": "Warning",
        "ExamEngine.Infrastructure.Outbox": "Warning",
        "ExamEngine.Infrastructure.Inbox": "Warning",
        "GameEngine.Infrastructure.Outbox": "Warning",
        "GameEngine.Infrastructure.Inbox": "Warning",
        "GameEngine.Infrastructure.LifeRegeneration": "Warning",
        "Users.Infrastructure.Outbox": "Warning",
        "Users.Infrastructure.Inbox": "Warning",
        "Subscription.Infrastructure.Outbox": "Warning",
        "Subscription.Infrastructure.Inbox": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "Revysr.Api"
    }
  },
  "CORS": {
    "AllowedOrigins": [
      "https://dev.revysr.com"
    ]
  },
  "Exam": {
    "OutboxWalStream": {
      "Enabled": true,
      "PublicationName": "exam_outbox_pub",
      "SlotName": "exam_outbox_slot",
      "ReconnectDelaySeconds": 5,
      "MaxReconnectDelaySeconds": 60
    },
    "InboxWalStream": {
      "Enabled": true,
      "PublicationName": "exam_inbox_pub",
      "SlotName": "exam_inbox_slot",
      "ReconnectDelaySeconds": 5,
      "MaxReconnectDelaySeconds": 60
    }
  },
  "Content": {
    "OutboxWalStream": {
      "Enabled": true,
      "PublicationName": "content_outbox_pub",
      "SlotName": "content_outbox_slot",
      "ReconnectDelaySeconds": 5,
      "MaxReconnectDelaySeconds": 60
    },
    "InboxWalStream": {
      "Enabled": true,
      "PublicationName": "content_inbox_pub",
      "SlotName": "content_inbox_slot",
      "ReconnectDelaySeconds": 5,
      "MaxReconnectDelaySeconds": 60
    }
  },
  "Game": {
    "OutboxWalStream": {
      "Enabled": true,
      "PublicationName": "game_outbox_pub",
      "SlotName": "game_outbox_slot",
      "ReconnectDelaySeconds": 5,
      "MaxReconnectDelaySeconds": 60
    },
    "InboxWalStream": {
      "Enabled": true,
      "PublicationName": "game_inbox_pub",
      "SlotName": "game_inbox_slot",
      "ReconnectDelaySeconds": 5,
      "MaxReconnectDelaySeconds": 60
    },
    "LifeRegeneration": {
      "IntervalInSeconds": 300,
      "BatchSize": 100
    }
  },
  "Users": {
    "OutboxWalStream": {
      "Enabled": true,
      "PublicationName": "users_outbox_pub",
      "SlotName": "users_outbox_slot",
      "ReconnectDelaySeconds": 5,
      "MaxReconnectDelaySeconds": 60
    },
    "InboxWalStream": {
      "Enabled": true,
      "PublicationName": "users_inbox_pub",
      "SlotName": "users_inbox_slot",
      "ReconnectDelaySeconds": 5,
      "MaxReconnectDelaySeconds": 60
    },
    "KeyCloak": {
      "AdminUrl": "http://localhost:8080/admin/realms/revysr/",
      "TokenUrl": "http://localhost:8080/realms/revysr/protocol/openid-connect/token",
      "ConfidentialClientId": "revysr-confidential-client",
      "ConfidentialClientSecret": "yoeyURMECMVfB6ppZI2c932EpjSSQOGS",
      "PublicClientId": "revysr-public-client"
    }
  },
  "Subscription": {
    "OutboxWalStream": {
      "Enabled": true,
      "PublicationName": "subscription_outbox_pub",
      "SlotName": "subscription_outbox_slot",
      "ReconnectDelaySeconds": 5,
      "MaxReconnectDelaySeconds": 60
    },
    "InboxWalStream": {
      "Enabled": true,
      "PublicationName": "subscription_inbox_pub",
      "SlotName": "subscription_inbox_slot",
      "ReconnectDelaySeconds": 5,
      "MaxReconnectDelaySeconds": 60
    },
    "Freemius": {
      "ProductId": "",
      "SecretKey": "",
      "Auth": "",
      "ApiUrl": "https://api.freemius.com/v1/",
      "CheckoutUrl": "https://checkout.freemius.com"
    }
  }
}
