{
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "ContentManagement.Infrastructure.Outbox": "Warning",
        "ContentManagement.Infrastructure.Inbox": "Warning",
        "ExamEngine.Infrastructure.Outbox": "Warning",
        "ExamEngine.Infrastructure.Inbox": "Warning",
        "GameEngine.Infrastructure.Outbox": "Warning",
        "GameEngine.Infrastructure.Inbox": "Warning",
        "GameEngine.Infrastructure.LifeRegeneration": "Warning",
        "Users.Infrastructure.Outbox": "Warning",
        "Users.Infrastructure.Inbox": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "Revysr.Api"
    }
  },
  "Exam": {
    "Outbox": {
      "IntervalInSeconds": 1,
      "BatchSize": 50
    },
    "Inbox": {
      "IntervalInSeconds": 1,
      "BatchSize": 50
    }
  },
  "Content": {
    "Outbox": {
      "IntervalInSeconds": 1,
      "BatchSize": 50
    },
    "Inbox": {
      "IntervalInSeconds": 1,
      "BatchSize": 50
    }
  },
  "Game": {
    "Outbox": {
      "IntervalInSeconds": 1,
      "BatchSize": 50
    },
    "Inbox": {
      "IntervalInSeconds": 1,
      "BatchSize": 50
    },
    "LifeRegeneration": {
      "IntervalInSeconds": 300,
      "BatchSize": 100
    }
  },
  "Users": {
    "Outbox": {
      "IntervalInSeconds": 1,
      "BatchSize": 50
    },
    "Inbox": {
      "IntervalInSeconds": 1,
      "BatchSize": 50
    }
  }
}

